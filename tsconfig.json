{
  "compilerOptions": {
    "module": "es2015",
    "target": "es6",
    "outDir": "./",
    "rootDir": "./src",
    "removeComments": true,
    "declaration": true,
    "strict": true,
    "moduleResolution": "node"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
  "typedocOptions": {
	  "entryPoints": "src",
	  "entryPointStrategy": "expand",
	  "out": "tempdocs",
	  "json": "tempdocs/data.json",
	  "githubPages": false
  }
}